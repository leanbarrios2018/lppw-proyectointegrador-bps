<div class="container rounded bg-light mt-4">
    <div class=" mt-3">
        <h3 class="mx-auto titulos" >Tecnicos</h3>
    </div>

  <table class="table table-hover">
    <thead>
      <tr>
        <th>Dni</th>
        <th>Nombre y Apellido</th>
        <th>Ver horas por orden</th>
      </tr>
    </thead>
    <tbody id="tbody">
        {{#each respuesta}}
        <tr>
            <td>{{dni}}</td>
            <th>{{nombrecompleto}}</th>
            <th><a class="btn btn-primary" href="/informes/horas/{{dni}}">Detalles</a></th>
        </tr>
        {{/each}}
    </tbody>
  </table>
</div>
